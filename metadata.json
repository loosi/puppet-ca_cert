{
  "name": "pcfens-ca_cert",
  "version": "1.4.0",
  "author": "pcfens",
  "summary": "Manage system CA certificates",
  "license": "Apache-2.0",
  "source": "https://github.com/pcfens/puppet-ca_cert",
  "project_page": "https://github.com/pcfens/puppet-ca_cert",
  "issues_url": "https://github.com/pcfens/puppet-ca_cert/issues",
  "description": "Manage CA certificates on Linux Operating Systems",
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5.0",
        "6.0"
      ]
    },
    {
      "operatingsystem": "Suse",
      "operatingsystemrelease": [
        "11.0",
        "12.0"
      ]
    },
    {
      "operatingsystem": "OpenSuSE",
      "operatingsystemrelease": [
        "13.2",
        "42.1"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "12.04",
        "14.04",
        "16.04"
      ]
    },
    {
      "operatingsystem": "Archlinux",
      "operatingsystemrelease": [
        "4.x"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.0.0 < 2016.3.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0 < 5.0.0"
    }
  ],
  "dependencies": [
    {"name":"puppetlabs/stdlib","version_requirement":">= 3.0.0 <5.0.0"}
  ]
}
